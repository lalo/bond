{
    "name": "Bond.JSON",
    "version": "1.0.0-*",
    "buildOptions": {
        "compile": {
            "include": [
                "../../../src/json/*.cs",
                "../../../src/json/expressions/json/*.cs",
                "../../../src/json/properties/*.cs",
                "../../../src/json/protocols/*.cs",
                "../../gen/AssemblyInfo_Generated.cs"
            ]
        },
        "debugType": "portable",
        "nowarn": [
            "CS1591"
        ],
        "warningsAsErrors": true,
        "xmlDoc": true
    },
    "configurations": {
        "debug": {
            "buildOptions": {
                "keyFile": "../../../build/internal/bond.snk",
                "optimize": false,
                "publicSign": true
            }
        },
        "release": {
            "buildOptions": {
                "keyFile": "../../../build/internal/bond.snk",
                "optimize": true,
                "publicSign": true
            }
        }
    },
    "dependencies": {
        "attributes": "1.0.0-*",
        "core": "1.0.0-*",
        "reflection": "1.0.0-*",
        "Newtonsoft.Json": {
            "version": "9.0.1"
        }
    },
    "frameworks": {
        "netstandard1.0": {
            "dependencies": {
                "NETStandard.Library": "1.6.0"
            }
        },
        "netstandard1.6": {
            "buildOptions": {
                "define": [
                    "SUPPORTS_BIGINTEGER"
                ]
            },
            "dependencies": {
                "NETStandard.Library": "1.6.0"
            }
        }
    }
}
